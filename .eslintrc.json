{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "tsx" : true
    } 
  },
  "env": {
    "browser": true,
    "node": true, 
    "es6": true
  },
  "extends": [
    "airbnb"
    // "eslint:recommend",
    // "plugin:react/recommended"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ],
  "rules": {
    "linebreak-style": 0
  }
}
